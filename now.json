{
    "version": 2,
    "name": "Wordplay",
    "public": false,
    "builds": [
      { "src": "package.json", "use": "@now/static-build" },
      { "src": "/src/server/index.js", "use": "@now/node-server" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "/src/server/index.js" },
      { "src": "^/static/(.*)", "dest": "/static/$1" }
    ]
  }